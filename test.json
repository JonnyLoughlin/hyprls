{
  "n": "____root____",
  "a": [
    { "k": "autogenerated", "v": "0" },
    { "k": "source", "v": "~/.config/hypr/monitors.conf" },
    { "k": "monitor", "v": ",preferred,auto,1" },
    {
      "k": "exec-once",
      "v": "hyprpm reload -n \u0026 ~/.config/waybar/spotify-receiver \u0026 waybar \u0026 fcitx5 \u0026 discord \u0026 spotify \u0026 caprine \u0026 element-desktop \u0026 firefox \u0026 ckb-next --background \u0026 /usr/lib/polkit-kde-authentication-agent-1 \u0026 bash -c 'killall hyprpaper; hyprpaper' \u0026"
    },
    { "k": "env", "v": "XCURSOR_SIZE,24" },
    { "k": "$mainMod", "v": "SUPER" },
    { "k": "$here", "v": "$HOME/.config/hypr" },
    { "k": "bind", "v": "$mainMod SHIFT, Return, exec, warp-terminal" },
    { "k": "bind", "v": "$mainMod, Return, exec, kitty" },
    { "k": "bind", "v": "$mainMod, Q, killactive," },
    { "k": "bind", "v": "$mainMod SHIFT, C, exit," },
    { "k": "bind", "v": "$mainMod, E, exec, neovide" },
    { "k": "bind", "v": "$mainMod, B, exec, firefox" },
    { "k": "bind", "v": "$mainMod, P, exec, ~/.config/rofi/query" },
    { "k": "bind", "v": "$mainMod SHIFT, Space, togglefloating," },
    {
      "k": "bind",
      "v": "$mainMod, D, exec, ~/.config/rofi/launchers/type-3/launcher.sh"
    },
    { "k": "bind", "v": "$mainMod, Y, exec, rofimoji" },
    { "k": "bind", "v": "$mainMod, V, togglesplit," },
    { "k": "bind", "v": "$mainMod, lock, exec, waylock" },
    {
      "k": "bind",
      "v": "$mainMod, U, exec, [workspace 6] kitty --hold fish -c up"
    },
    { "k": "bind", "v": "$mainMod, left, movefocus, l" },
    { "k": "bind", "v": "$mainMod, h, movefocus, l" },
    { "k": "bind", "v": "$mainMod, right, movefocus, r" },
    { "k": "bind", "v": "$mainMod, l, movefocus, r" },
    { "k": "bind", "v": "$mainMod, up, movefocus, u" },
    { "k": "bind", "v": "$mainMod, k, movefocus, u" },
    { "k": "bind", "v": "$mainMod, down, movefocus, d" },
    { "k": "bind", "v": "$mainMod, j, movefocus, d" },
    { "k": "bind", "v": "$mainMod, ampersand, workspace, 1" },
    { "k": "bind", "v": "$mainMod, eacute, workspace, 2" },
    { "k": "bind", "v": "$mainMod, quotedbl, workspace, 3" },
    { "k": "bind", "v": "$mainMod, apostrophe, workspace, 4" },
    { "k": "bind", "v": "$mainMod, parenleft, workspace, 5" },
    { "k": "bind", "v": "$mainMod, minus, workspace, 6" },
    { "k": "bind", "v": "$mainMod, egrave, workspace, 7" },
    { "k": "bind", "v": "$mainMod, underscore, workspace, 8" },
    { "k": "bind", "v": "$mainMod, ccedilla, workspace, 9" },
    { "k": "bind", "v": "$mainMod, agrave, workspace, 10" },
    { "k": "bind", "v": "$mainMod SHIFT, ampersand, movetoworkspace, 1" },
    { "k": "bind", "v": "$mainMod SHIFT, eacute, movetoworkspace, 2" },
    { "k": "bind", "v": "$mainMod SHIFT, quotedbl, movetoworkspace, 3" },
    { "k": "bind", "v": "$mainMod SHIFT, apostrophe, movetoworkspace, 4" },
    { "k": "bind", "v": "$mainMod SHIFT, parenleft, movetoworkspace, 5" },
    { "k": "bind", "v": "$mainMod SHIFT, minus, movetoworkspace, 6" },
    { "k": "bind", "v": "$mainMod SHIFT, egrave, movetoworkspace, 7" },
    { "k": "bind", "v": "$mainMod SHIFT, underscore, movetoworkspace, 8" },
    { "k": "bind", "v": "$mainMod SHIFT, ccedilla, movetoworkspace, 9" },
    { "k": "bind", "v": "$mainMod SHIFT, agrave, movetoworkspace, 10" },
    {
      "k": "bind",
      "v": "$mainMod CTRL, left, movecurrentworkspacetomonitor, l"
    },
    {
      "k": "bind",
      "v": "$mainMod CTRL, right, movecurrentworkspacetomonitor, r"
    },
    { "k": "bind", "v": "$mainMod, mouse_down, workspace, e+1" },
    { "k": "bind", "v": "$mainMod, mouse_up, workspace, e-1" },
    { "k": "bindm", "v": "$mainMod, mouse:272, movewindow" },
    { "k": "bindm", "v": "$mainMod, mouse:273, resizewindow" },
    { "k": "bind", "v": "$mainMod, T, togglegroup" },
    { "k": "bind", "v": "$mainMod SHIFT, tab, changegroupactive, b" },
    { "k": "bind", "v": "$mainMod, tab, changegroupactive, f" },
    { "k": "binde", "v": "$mainMod, xf86monbrightnessup, exec, brillo -A 5" },
    { "k": "binde", "v": "$mainMod, xf86monbrightnessdown, exec, brillo -U 5" },
    { "k": "binde", "v": ", xf86monbrightnessup, exec, brillo -A 10" },
    { "k": "binde", "v": ", xf86monbrightnessdown, exec, brillo -U 10" },
    { "k": "binde", "v": "SHIFT, xf86monbrightnessup, exec, brillo -A 20" },
    { "k": "binde", "v": "SHIFT, xf86monbrightnessdown, exec, brillo -U 20" },
    {
      "k": "binde",
      "v": ", xf86audioraisevolume, exec, $here/volume_brightness.sh volume_up"
    },
    {
      "k": "binde",
      "v": ", xf86audiolowervolume, exec, $here/volume_brightness.sh volume_down"
    },
    {
      "k": "binde",
      "v": ", xf86audiomute, exec, $here/volume_brightness.sh volume_mute"
    },
    { "k": "bind", "v": ", xf86audionext, exec, playerctl next" },
    { "k": "bind", "v": ", xf86audioprev, exec, playerctl previous" },
    { "k": "bind", "v": ", xf86audioplay, exec, playerctl play-pause" },
    { "k": "bind", "v": ", xf86audiostop, exec, rofi-spotify --like-current" },
    {
      "k": "bind",
      "v": "SHIFT, xf86audiostop, exec, rofi-spotify --add-to-playlist"
    },
    { "k": "bind", "v": ", print, exec, hyprshot -m output" },
    { "k": "bind", "v": "SHIFT, print, exec, hyprshot -m region" },
    { "k": "bind", "v": "$mainMod ALT, u, exec, rofimoji -a unicode" },
    { "k": "bind", "v": "$mainMod, F, fullscreen" },
    { "k": "bind", "v": "$mainMod SHIFT, F, fullscreen, 1" },
    { "k": "bind", "v": "$mainMod SHIFT, equal, movetoworkspace, special" },
    { "k": "bind", "v": "$mainMod, equal, togglespecialworkspace" },
    { "k": "bind", "v": "$mainMod, A, hyprexpo:expo, toggle" },
    {
      "k": "windowrulev2",
      "v": "opacity 0.8 override 0.6 override,class:(kitty)"
    },
    {
      "k": "windowrulev2",
      "v": "opacity 0.8 override 0.6 override,class:(neovide)"
    },
    { "k": "windowrulev2", "v": "opacity 1 override 1 override,class:(obs)" },
    { "k": "windowrulev2", "v": "tile,class:(dev.warp.Warp)" },
    { "k": "windowrulev2", "v": "workspace 9 silent,class:(Spotify)" },
    { "k": "windowrulev2", "v": "workspace 10 silent,class:(Element)" },
    { "k": "windowrulev2", "v": "group set,class:(Element)" },
    { "k": "windowrulev2", "v": "workspace 10 silent,class:(Caprine)" },
    { "k": "windowrulev2", "v": "group set,class:(Caprine)" },
    { "k": "windowrulev2", "v": "workspace 10 silent,class:(discord)" },
    { "k": "windowrulev2", "v": "group set,class:(discord)" },
    {
      "k": "windowrulev2",
      "v": "workspace 3 silent,class:(^MATLAB),title:(^Figure \\d: )"
    },
    {
      "k": "windowrulev2",
      "v": "workspace 3 silent,class:(Backend),title:(\\[dev\\])"
    },
    { "k": "windowrulev2", "v": "stayfocused,class:(Rofi)" },
    { "k": "windowrulev2", "v": "tile,class:(qemu-system-x86_64)" },
    { "k": "windowrulev2", "v": "tile,class:(Pianoteq),title:(^Pianoteq)" },
    { "k": "windowrulev2", "v": "tile,class:(^MATLAB),title:(^Figure \\d: )" },
    {
      "k": "windowrulev2",
      "v": "stayfocused,class:(kwalletd5),title:(^KDE Wallet Service$)"
    }
  ],
  "s": [
    {
      "n": "misc",
      "a": [
        { "k": "enable_swallow", "v": "true" },
        { "k": "swallow_regex", "v": "^kitty$" }
      ],
      "s": []
    },
    {
      "n": "input",
      "a": [
        { "k": "kb_layout", "v": "fr" },
        { "k": "kb_variant", "v": "" },
        { "k": "kb_model", "v": "" },
        { "k": "kb_options", "v": "compose:rwin" },
        { "k": "kb_rules", "v": "" },
        { "k": "follow_mouse", "v": "1" },
        { "k": "sensitivity", "v": "0" }
      ],
      "s": [
        {
          "n": "touchpad",
          "a": [
            { "k": "natural_scroll", "v": "yes" },
            { "k": "scroll_factor", "v": "0.2" }
          ],
          "s": []
        }
      ]
    },
    {
      "n": "general",
      "a": [
        { "k": "gaps_in", "v": "5" },
        { "k": "gaps_out", "v": "20" },
        { "k": "border_size", "v": "2" },
        {
          "k": "col.active_border",
          "v": "rgba(33ccffee) rgba(00ff99ee) 45deg"
        },
        { "k": "col.inactive_border", "v": "rgba(595959aa)" },
        { "k": "layout", "v": "dwindle" }
      ],
      "s": []
    },
    {
      "n": "decoration",
      "a": [
        { "k": "rounding", "v": "10" },
        { "k": "active_opacity", "v": "0.9" },
        { "k": "inactive_opacity", "v": "0.7" }
      ],
      "s": [
        {
          "n": "blur",
          "a": [
            { "k": "enabled", "v": "true" },
            { "k": "size", "v": "10" },
            { "k": "ignore_opacity", "v": "true" },
            { "k": "xray", "v": "true" },
            { "k": "passes", "v": "2" }
          ],
          "s": []
        }
      ]
    },
    {
      "n": "animations",
      "a": [
        { "k": "enabled", "v": "yes" },
        { "k": "bezier", "v": "myBezier, 0.05, 0.9, 0.1, 1.05" },
        { "k": "animation", "v": "windows, 1, 7, myBezier" },
        { "k": "animation", "v": "windowsOut, 1, 7, default, popin 80%" },
        { "k": "animation", "v": "border, 1, 10, default" },
        { "k": "animation", "v": "borderangle, 1, 8, default" },
        { "k": "animation", "v": "fade, 1, 7, default" },
        { "k": "animation", "v": "workspaces, 1, 6, default" }
      ],
      "s": []
    },
    {
      "n": "dwindle",
      "a": [
        { "k": "pseudotile", "v": "yes" },
        { "k": "preserve_split", "v": "yes" },
        { "k": "force_split", "v": "2" }
      ],
      "s": []
    },
    { "n": "master", "a": [{ "k": "new_is_master", "v": "true" }], "s": [] },
    {
      "n": "gestures",
      "a": [
        { "k": "workspace_swipe", "v": "on" },
        { "k": "workspace_swipe_distance", "v": "3000" }
      ],
      "s": []
    },
    {
      "n": "plugin",
      "a": [],
      "s": [
        {
          "n": "hyprexpo",
          "a": [
            { "k": "columns", "v": "3" },
            { "k": "gap_size", "v": "5" },
            { "k": "bg_col", "v": "rgb(111111)" },
            { "k": "workspace_method", "v": "first 1" },
            { "k": "enable_gesture", "v": "true" },
            { "k": "gesture_distance", "v": "300" },
            { "k": "gesture_positive", "v": "true" }
          ],
          "s": []
        }
      ]
    }
  ]
}
